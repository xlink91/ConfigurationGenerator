<?xml version="1.0"?>
<package >
  <metadata>
    <id>ConfigFileGenerator</id>
    <version>1.0.2</version>
    <title>Configuration File Generator</title>
    <authors>DHT</authors>
    <owners>DHT</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>Handling configuration files easily</description>
    <releaseNotes></releaseNotes>
    <copyright>Copyright 2019</copyright>
    <tags>Configuration</tags>
    <licenseUrl>https://www.opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>https://github.com/xlink91/ConfigurationGenerator</projectUrl>
    <dependencies>
        <dependency id="Newtonsoft.Json" version="12.0.1" />
    </dependencies>
  </metadata>   
  <files>
    <file src="scripts\*.*" target="tools" />
    <file src="ConfigFiles\" target="content\ConfigFiles" />
    <file src="bin\Release\ConfigFileGenerator.pdb" target="lib\net461" />
    <file src="bin\Release\ConfigFileGenerator.dll" target="lib\net461" />
    <file src="bin\Release\*.*" target="tools" exclude="bin\Release\ConfigFileGenerator.pdb; bin\Release\ConfigFileGenerator.dll;"/>
    <file src="bin\Release\Roslyn\*.*" target="tools\roslyn" />
  </files>
</package>